
config qmp 'interfaces'
	option mesh_devices 'eth0 wlan0'
	option lan_devices 'eth0'

config qmp 'node'
	option primary_device 'eth0'
	option key '/tmp/qmp_key'
	option community_node_id '8a79'
	option latitude '-75.68945'
	option longitude '4.72359'
	option elevation '1660'
	option contact 'proyectos@vivirenlafinca.org'
	option community_id 'VivirEnLaFinca'

config qmp 'services'
	option vpn '0'
	option captive_portal '0'
	option b6m '0'
	option libremap '1'
	option gwck '1'
	option auto_upgrade '0'
	option mesh_dns '1'
	option bwtest '1'
	option munin '1'

config qmp 'update'
	option url 'http://fw.qmp.cat/master'
	option images 'IMAGES'
	option devices 'DEVICES'
	option filter 'sysupgrade'
	option preserve '/etc/config/qmp /etc/shadow /etc/passwd /etc/rc.local /etc/firewall.user /etc/dropbear /etc/config/b6m-spread /etc/qmp/.birth /etc/config/gateways /etc/config/mini_snmpd'

config qmp 'roaming'
	option dhcp_offset '2'
	option qmp_leasetime '1h'
	option ignore '1'

config qmp 'networks'
	option dns '10.40.2.254'
	option disable_lan_dhcp '0'
	option disable_mesh_dhcp '1'
	option rescue_prefix24 '169.254'
	option bmx6_ipv4_prefix24 '10.202.0'
	option mesh_protocol_vids 'bmx6:12'
	option bmx6_mesh_prefix48 'fd02:0:0'
	option lan_ula_prefix48 'fd00:1714:1714'
	option publish_lan '1'
	option bmx6_ripe_prefix48 '2012:0:0'
	option lan_address '10.40.2.1'
	option bmx6_ipv4_address '10.40.2.1/24'
	option lan_netmask '255.255.255.0'

config qmp 'wireless'
	option driver 'mac80211'
	option country 'US'
	option bssid '02:CA:FF:EE:BA:BE'
	option mrate '6000'

config wireless
	option mac '24:a4:3c:ac:8e:e2'
	option device 'wlan0'
	option mode 'adhoc_ap'
	option name 'NuestraRED'
	option channel '6+'
	option txpower '30'
	option essidap 'NuestraRED.org'
